                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" asp-controller="Product" asp-action="Mobile">Mobile & Tablet</a>
                        <a class="dropdown-item" asp-controller="Product" asp-action="Laptop">Labtop </a>
                        <a class="dropdown-item" asp-controller="Product" asp-action="Screen">Screen </a>
                        <a class="dropdown-item" asp-controller="Product" asp-action="SmartWatch">Smart Watch</a>
                    </div>
                </li>

                <li class="nav-item">
                    <form asp-controller="Product" asp-action="Search" method="get" class="form-inline my-2 my-lg-0">
                        <input class="form-control mr-sm-5 custom-search-width" style="width: 500px;" type="text"
                               placeholder="Search For Products Or brands" name="name">
                        <button class="btn btn-outline-success my-2 my-sm-0 d-none" type="submit">Search</button>
                    </form>
                </li>



                <li class="nav-item">
                  <a class="nav-link mr-sm-5 fas fa-user" asp-area="" asp-controller="Account" asp-action="Login">My account</a>

                </li>

                <li class="nav-item">
                    <a class="nav-link mr-sm-2" href="#"><i class="fas fa-shopping-cart"></i> Cart</a>
                </li>

                @if (User.Identity.IsAuthenticated)
                {
                    <li class="nav-item">
                        <a class="nav-link mr-sm-5 " asp-area="" asp-controller="Account" asp-action="Logout">Logout</a>

                    </li>
                }
            </ul>
        </div>

